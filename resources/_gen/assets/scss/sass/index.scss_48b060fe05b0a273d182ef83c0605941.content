@media all{body{background:default;color:#1f2833;font-family:quicksand,sans-serif}a{text-decoration:none;color:#81a2be}a:hover{color:#c66}p{margin-top:0;font-size:15px;line-height:1.5em;padding:.1em .7em}h1{color:#45a29e}h2{margin-top:0;font-size:32px}h3,h4{padding:.7em;margin-bottom:.7em;margin-top:0;font-size:15px;color:#45a29e;border:2px solid #c5c6c7;border-left:none;border-right:none}h5{margin:0;padding:0;padding-left:.7em;font-size:15px;color:#45a29e}ul{font-size:15px;list-style-type:square;color:#81a2be;padding-left:15px;margin:0 0 .7em;padding:4px 0 4px .7em;list-style:none}ul .fact-list{color:#1f2833}ul .fact-list-description{color:#61892f}ul li{margin-bottom:2px}.main-grid{display:grid;grid-template-columns:auto;grid-template-rows:auto auto auto auto auto;grid-template-areas:"header" "jumbotron" "picture" "about" "main";margin:0 auto;grid-row-gap:4px}}@media only all and (min-width:770px){.main-grid{grid-template-columns:500px 245px;grid-template-rows:auto auto auto auto;grid-template-areas:"header header" "jumbotron picture" "main about" "main goal";max-width:800px;grid-column-gap:1.2em}}@media only all{.main-grid{grid-template-columns:530px 245px;grid-template-rows:auto auto auto auto;grid-template-areas:"header header" "jumbotron picture" "main about" "main goal"}}@media all{header{grid-area:header;border-bottom:2px solid #c5c6c7}header h1{margin-bottom:0;font-size:4em}main{grid-area:main}main article p:last-child{margin-bottom:25px}.jumbotron{grid-area:jumbotron}.jumbotron .center{margin:2em 0 1em;text-align:center}}@media all and (min-width:770px){.jumbotron .center{margin-top:3.5em}}@media all{.jumbotron h2{margin-top:.2em;margin-bottom:0}.jumbotron h4{border:none;padding:0}.jumbotron p{width:75%;margin:auto}.picture{grid-area:picture;text-align:center}.picture img{border-radius:50%;margin:1em 0}.about{grid-area:about}.about h5{margin:0;padding:0;padding-left:.7em}.goal{grid-area:goal}}@media only screen{.fullhref{display:none}}@media only print{.hide-on-print{display:none}.fullhref{display:inline}}